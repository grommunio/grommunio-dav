{
    "name"                  : "grommunio/dav",
    "description"           : "This is grommunio's DAV implementation",
    "require": {
        "sabre/dav"         : "~4.3.1",
        "apache/log4php"    : "~2.3.0",
        "psr/log"           : "^1.1.4"
    },
    "require-dev": {
        "phpunit/phpunit"   : "^9"
    },
    "license"               : "AGPL v3",
    "authors": [
        {
            "name"          : "Grommunio developers",
            "email"         : "dev@grommunio.com",
            "role"          : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "grommunio\\DAV\\" : "lib/"
        },
        "classmap"          : ["/usr/share/php-mapi/"],
